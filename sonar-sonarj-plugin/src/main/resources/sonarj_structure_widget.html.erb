<%
if measure('sonarj.acd')
%>
<div class="dashbox">
  <table>
    <tr>
        <td width="190px"><h3>Relative Package Cyclicity</h3></td>
        <td><h3>ACD (John Lakos)</h3></td>
    </tr>
    <tr>
        <td>
            <span class="big">
              <%= format_measure('sonarj.relative_cyclicity') -%> <%= tendency_icon(measure('sonarj.relative_cyclicity')) %> 
            </span>
        </td>
        <td>
            <span class="big">
              <%= format_measure('sonarj.acd') -%> <%= tendency_icon(measure('sonarj.acd')) %> 
            </span>
        </td>
    </tr>
    <tr>
        <td><h3>Biggest Cycle Group Size</h3></td>
        <td><h3>NCCD (John Lakos)</h3></td>
    </tr>
   <tr>
        <td>
            <span class="big">
              <%= format_measure('sonarj.biggest_cycle_group') -%> <%= tendency_icon(measure('sonarj.biggest_cycle_group')) %>
            </span>
        </td>
        <td>
            <span class="big">
              <%= format_measure('sonarj.nccd') -%> <%= tendency_icon(measure('sonarj.nccd')) %>
            </span>
        </td>
    </tr>
    <tr>
        <td>
              <%= format_measure('sonarj.cyclic_packages') -%> <%= tendency_icon(measure('sonarj.cyclic_packages')) %> Cyclic Packages
        </td>
        <td>
              <%= format_measure('sonarj.packages') -%> <%= tendency_icon(measure('sonarj.packages')) %> Packages
        </td>
    </tr>
   <tr>
        <td>
              <%= format_measure('sonarj.cyclicity') -%> <%= tendency_icon(measure('sonarj.cyclicity')) %> Package Cyclicity
        </td>
        <td>
              <%= format_measure('sonarj.instructions') -%> <%= tendency_icon(measure('sonarj.instructions')) %> Byte Code Instr.
        </td>
    </tr>
  </table>
</div>
<% 
elsif measure('sonarj.cycle_group_size') 
%>
<div class="dashbox">
  <h3>Part of Cycle Group of</h3>
  <p>
    <span class="big">
      <%= format_measure('sonarj.cycle_group_size') -%> <%= tendency_icon(measure('sonarj.cycle_group_size')) %> Packages
    </span>
  </p>  
</div>
<%
end
%>  