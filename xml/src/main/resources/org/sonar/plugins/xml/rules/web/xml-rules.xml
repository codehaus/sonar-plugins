<profile>
  <name>Default XML Profile</name>
  <language>web</language>
  <rules>

    <rule>
      <repositoryKey>Xml</repositoryKey>
      <key>XPathCheck</key>
    </rule>
    <rule>
      <repositoryKey>Xml</repositoryKey>
      <key>XmlSchemaCheck</key>
      <parameters>
        <parameter>
          <key>schemas</key>
          <value>autodetect</value>
        </parameter>
      </parameters>
    </rule>
    
  </rules>

</profile>