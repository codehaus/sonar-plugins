<%
if measure('sonarj_acd')
%>
<div class="dashbox">
  <table>
    <tr>
        <td width="190px"><h3>Relative Package Cyclicity</h3></td>
        <td width="160px"><h3>ACD (John Lakos)</h3></td>
        <td align="right"><a href="http://www.hello2morrow.com/products/sonarj/sonar#structure" target="_blank"><%= image_tag 'help.png', :class => 'png' -%></a></td>
    </tr>
    <tr>
        <td>
            <span class="big">
              <%= format_measure('sonarj_relative_cyclicity') -%> <%= tendency_icon(measure('sonarj_relative_cyclicity')) %> 
            </span>
        </td>
        <td>
            <span class="big">
              <%= format_measure('sonarj_acd') -%> <%= tendency_icon(measure('sonarj_acd')) %> 
            </span>
        </td>
    </tr>
   	<tr>
        <td>
              <%= format_measure('sonarj_biggest_cycle_group', :suffix => ' biggest cycle group size') -%> <%= tendency_icon(measure('sonarj_biggest_cycle_group')) %>
        </td>
        <td>
              <%= format_measure('sonarj_nccd', :suffix => ' NCCD (John Lakos)') -%> <%= tendency_icon(measure('sonarj_nccd')) %>
        </td>
    </tr>
    <tr>
        <td>
              <%= format_measure('sonarj_cyclic_packages', :suffix => ' cyclic packages') -%> <%= tendency_icon(measure('sonarj_cyclic_packages')) %>
        </td>
        <td>
              <%= format_measure('sonarj_packages', :suffix => ' packages') -%> <%= tendency_icon(measure('sonarj_packages')) %>
        </td>
    </tr>
   <tr>
        <td>
              <%= format_measure('sonarj_cyclicity', :suffix => ' package cyclicity') -%> <%= tendency_icon(measure('sonarj_cyclicity')) %>
        </td>
        <td>
              <%= format_measure('sonarj_instructions', :suffix => ' byte code instr.') -%> <%= tendency_icon(measure('sonarj_instructions')) %>
        </td>
    </tr>
  </table>
</div>
