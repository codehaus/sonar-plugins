<rules>

   <rule key="SYNC_LOOP">
      <name><![CDATA[Reliability - Synchronized method can cause deadlock.]]></name>
      <configKey><![CDATA[SYNC_LOOP]]></configKey>
      <category name="Reliability"/>
      <description></description>
   </rule>


   <rule key="LOOP">
      <name><![CDATA[Reliability - Method invocation forms loop in dependency graph.]]></name>
      <configKey><![CDATA[LOOP]]></configKey>
      <category name="Reliability"/>
      <description></description>
   </rule>


   <rule key="LOCK">
      <name><![CDATA[Reliability - Possible deadlock condition]]></name>
      <configKey><![CDATA[LOCK]]></configKey>
      <category name="Reliability"/>
      <description></description>
   </rule>


   <rule key="WAIT">
      <name><![CDATA[Reliability - Possible deadlock on wait()]]></name>
      <configKey><![CDATA[WAIT]]></configKey>
      <category name="Reliability"/>
      <description></description>
   </rule>


   <rule key="LOCKLIST">
      <name><![CDATA[Reliability - Holding lock(s)]]></name>
      <configKey><![CDATA[LOCKLIST]]></configKey>
      <category name="Reliability"/>
      <description></description>
   </rule>


   <rule key="WAIT_PATH">
      <name><![CDATA[Reliability - Call to method  can cause deadlock]]></name>
      <configKey><![CDATA[WAIT_PATH]]></configKey>
      <category name="Reliability"/>
      <description></description>
   </rule>


   <rule key="NOSYNC">
      <name><![CDATA[Reliability - Synchronized method overridden by non-synchronized method]]></name>
      <configKey><![CDATA[NOSYNC]]></configKey>
      <category name="Reliability"/>
      <description></description>
   </rule>


   <rule key="CONCURRENT_CALL">
      <name><![CDATA[Reliability - Method should be synchronized.]]></name>
      <configKey><![CDATA[CONCURRENT_CALL]]></configKey>
      <category name="Reliability"/>
      <description></description>
   </rule>


   <rule key="CONCURRENT_ACCESS">
      <name><![CDATA[Reliability - Field should be volatile.]]></name>
      <configKey><![CDATA[CONCURRENT_ACCESS]]></configKey>
      <category name="Reliability"/>
      <description></description>
   </rule>


   <rule key="RUN_NOSYNC">
      <name><![CDATA[Reliability - Runnable Method should be synchronized.]]></name>
      <configKey><![CDATA[RUN_NOSYNC]]></configKey>
      <category name="Reliability"/>
      <description></description>
   </rule>


   <rule key="LOCK_ASSIGN">
      <name><![CDATA[Reliability - Invalid change of lock]]></name>
      <configKey><![CDATA[LOCK_ASSIGN]]></configKey>
      <category name="Reliability"/>
      <description></description>
   </rule>


   <rule key="LOCK_ASSIGN2">
      <name><![CDATA[Reliability - Invalid change of lock (potential ownership)]]></name>
      <configKey><![CDATA[LOCK_ASSIGN2]]></configKey>
      <category name="Reliability"/>
      <description></description>
   </rule>


   <rule key="WAIT_NOSYNC">
      <name><![CDATA[Correctness - Method called without synchronization]]></name>
      <configKey><![CDATA[WAIT_NOSYNC]]></configKey>
      <category name="Reliability"/>
      <description></description>
   </rule>


   <rule key="NOT_OVERRIDDEN">
      <name><![CDATA[Bad Practice - Method with same name not overridden in derived class]]></name>
      <configKey><![CDATA[NOT_OVERRIDDEN]]></configKey>
      <category name="Reliability"/>
      <description></description>
   </rule>


   <rule key="EQUALS_NOT_OVERRIDDEN">
      <name><![CDATA[Bad Practice - hashCode() was overridden but not equals()]]></name>
      <configKey><![CDATA[EQUALS_NOT_OVERRIDDEN]]></configKey>
      <category name="Reliability"/>
      <description></description>
   </rule>


   <rule key="HASHCODE_NOT_OVERRIDDEN">
      <name><![CDATA[Bad Practice - equals() was overridden but not hashCode()]]></name>
      <configKey><![CDATA[HASHCODE_NOT_OVERRIDDEN]]></configKey>
      <category name="Reliability"/>
      <description></description>
   </rule>


   <rule key="FIELD_REDEFINED">
      <name><![CDATA[Bad Practice - Component  in class  shadows one in base class]]></name>
      <configKey><![CDATA[FIELD_REDEFINED]]></configKey>
      <category name="Reliability"/>
      <description></description>
   </rule>


   <rule key="SHADOW_LOCAL">
      <name><![CDATA[Correctness - Local variable  shadows component of class]]></name>
      <configKey><![CDATA[SHADOW_LOCAL]]></configKey>
      <category name="Reliability"/>
      <description></description>
   </rule>


   <rule key="SUPER_FINALIZE">
      <name><![CDATA[Correctness - Method finalize() doesn't call super.finalize()]]></name>
      <configKey><![CDATA[SUPER_FINALIZE]]></configKey>
      <category name="Reliability"/>
      <description></description>
   </rule>


   <rule key="NULL_PARAM">
      <name><![CDATA[Reliability - Method can be invoked with NULL]]></name>
      <configKey><![CDATA[NULL_PARAM]]></configKey>
      <category name="Reliability"/>
      <description></description>
   </rule>


   <rule key="NULL_VAR">
      <name><![CDATA[Reliability - Value of referenced variable may be NULL]]></name>
      <configKey><![CDATA[NULL_VAR]]></configKey>
      <category name="Reliability"/>
      <description></description>
   </rule>


   <rule key="NULL_PTR">
      <name><![CDATA[Reliability - NULL reference can be used]]></name>
      <configKey><![CDATA[NULL_PTR]]></configKey>
      <category name="Reliability"/>
      <description></description>
   </rule>


   <rule key="ZERO_OPERAND">
      <name><![CDATA[Correctness - Zero operand for operation]]></name>
      <configKey><![CDATA[ZERO_OPERAND]]></configKey>
      <category name="Reliability"/>
      <description></description>
   </rule>


   <rule key="ZERO_RESULT">
      <name><![CDATA[Correctness - Result of operation is always 0]]></name>
      <configKey><![CDATA[ZERO_RESULT]]></configKey>
      <category name="Reliability"/>
      <description></description>
   </rule>


   <rule key="SHIFT_COUNT">
      <name><![CDATA[Correctness - Shift with count]]></name>
      <configKey><![CDATA[SHIFT_COUNT]]></configKey>
      <category name="Reliability"/>
      <description></description>
   </rule>


   <rule key="SHIFT_RANGE">
      <name><![CDATA[Correctness - Shift count range is out of domain]]></name>
      <configKey><![CDATA[SHIFT_RANGE]]></configKey>
      <category name="Reliability"/>
      <description></description>
   </rule>


   <rule key="CONVERSION">
      <name><![CDATA[Correctness - Expression value has no intersection with type domain.]]></name>
      <configKey><![CDATA[CONVERSION]]></configKey>
      <category name="Reliability"/>
      <description></description>
   </rule>


   <rule key="TRUNCATION">
      <name><![CDATA[Correctness - Data can be lost as a result of truncation]]></name>
      <configKey><![CDATA[TRUNCATION]]></configKey>
      <category name="Reliability"/>
      <description></description>
   </rule>


   <rule key="OVERFLOW">
      <name><![CDATA[Reliability - Type cast may not correctly applied]]></name>
      <configKey><![CDATA[OVERFLOW]]></configKey>
      <category name="Reliability"/>
      <description></description>
   </rule>


   <rule key="SAME_RESULT">
      <name><![CDATA[Correctness - Comparison always produces the same result]]></name>
      <configKey><![CDATA[SAME_RESULT]]></configKey>
      <category name="Efficiency"/>
      <description></description>
   </rule>


   <rule key="DISJOINT_MASK">
      <name><![CDATA[Correctness - Compared expressions can be equal only when 0]]></name>
      <configKey><![CDATA[DISJOINT_MASK]]></configKey>
      <category name="Reliability"/>
      <description></description>
   </rule>


   <rule key="NO_EFFECT">
      <name><![CDATA[Correctness - Reminder always equal to the first operand]]></name>
      <configKey><![CDATA[NO_EFFECT]]></configKey>
      <category name="Efficiency"/>
      <description></description>
   </rule>


   <rule key="SHORT_CHAR_CMP">
      <name><![CDATA[Correctness - Comparison of short with char]]></name>
      <configKey><![CDATA[SHORT_CHAR_CMP]]></configKey>
      <category name="Reliability"/>
      <description></description>
   </rule>


   <rule key="STRING_CMP">
      <name><![CDATA[Correctness - Compare strings as object references]]></name>
      <configKey><![CDATA[STRING_CMP]]></configKey>
      <category name="Efficiency"/>
      <description></description>
   </rule>


   <rule key="WEAK_CMP">
      <name><![CDATA[Correctness - Replace inequality comparison with equality]]></name>
      <configKey><![CDATA[WEAK_CMP]]></configKey>
      <category name="Maintainability"/>
      <description></description>
   </rule>


   <rule key="INCOMP_CASE">
      <name><![CDATA[Correctness - Switch case constant can't be produced by switch expression]]></name>
      <configKey><![CDATA[INCOMP_CASE]]></configKey>
      <category name="Reliability"/>
      <description></description>
   </rule>


   <rule key="NEG_LEN">
      <name><![CDATA[Security - Array length is less than zero]]></name>
      <configKey><![CDATA[NEG_LEN]]></configKey>
      <category name="Reliability"/>
      <description></description>
   </rule>


   <rule key="MAYBE_NEG_LEN">
      <name><![CDATA[Security - Array length  may be less than zero]]></name>
      <configKey><![CDATA[MAYBE_NEG_LEN]]></configKey>
      <category name="Reliability"/>
      <description></description>
   </rule>


   <rule key="BAD_INDEX">
      <name><![CDATA[Security - Index is out of array bounds]]></name>
      <configKey><![CDATA[BAD_INDEX]]></configKey>
      <category name="Reliability"/>
      <description></description>
   </rule>


   <rule key="MAYBE_BAD_INDEX">
      <name><![CDATA[Security - Index may be out of array bounds]]></name>
      <configKey><![CDATA[MAYBE_BAD_INDEX]]></configKey>
      <category name="Reliability"/>
      <description></description>
   </rule>

</rules>
