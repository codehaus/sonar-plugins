  <ciManagement>
    <system>BAMBOO</system>
    <url>http://bamboo.ci.codehaus.org/browse/xxx</url>
  </ciManagement>
  <scm>
    <connection>scm:svn:http://svn.codehaus.org/sonar-plugins/trunk/comparing</connection>
    <developerConnection>scm:svn:https://svn.codehaus.org/sonar-plugins/trunk/comparing</developerConnection>
    <url>http://svn.sonar-plugins.codehaus.org</url>
  </scm>
  
  <properties>
    <license.owner>David FRANCOIS</license.owner>
    <sonar.version>3.2</sonar.version>
    <sonar.pluginName>Comparing</sonar.pluginName>
    <sonar.pluginClass>org.sonar.plugins.comparing.ComparingPlugin</sonar.pluginClass>
  </properties>
  
  <dependencies>
    <dependency>
      <groupId>org.codehaus.sonar</groupId>
      <artifactId>sonar-plugin-api</artifactId>
      <version>${sonar.version}</version>
      <scope>provided</scope>
    </dependency>
    
    <dependency>
      <groupId>org.codehaus.sonar</groupId>
      <artifactId>sonar-testing-harness</artifactId>
      <version>${sonar.version}</version>
      <scope>test</scope>
    </dependency>
  </dependencies>
  
</project>
