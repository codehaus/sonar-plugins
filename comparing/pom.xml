  <artifactId>sonar-comparing-plugin</artifactId>
  <packaging>sonar-plugin</packaging>
  <version>0.1-SNAPSHOT</version>
  <name>Sonar Comparing Plugin</name>
  <description>This plugin aggregates differents metrics and compare the project value with this.</description>
  <url>http://docs.codehaus.org/display/SONAR/Comparing+plugin</url>
  
  <inceptionYear>2012</inceptionYear>

  <organization>
    <name>Codehaus Sonar Plugins</name>
    <url>http://sonar-plugins.codehaus.org/</url>
  </organization>

  <developers>
    <developer>
      <id>francoisd</id>
      <name>David FRANCOIS</name>
    </developer>
  </developers>
  
  <issueManagement>
    <system>JIRA</system>
    <url>http://jira.codehaus.org/browse/SONARPLUGINS/component/xxx</url>
