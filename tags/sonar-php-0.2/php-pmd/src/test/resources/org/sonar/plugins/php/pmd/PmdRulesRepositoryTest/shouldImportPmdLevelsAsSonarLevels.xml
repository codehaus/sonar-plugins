<ruleset>
    <description>Sonar PMD rules</description>
    <rule name="CyclomaticComplexity">
        <priority>3</priority>
        <properties>
            <property name="minimum" value="30"/>
        </properties>
    </rule>
    <rule name="ExcessiveClassLength">
        <priority>4</priority>
    </rule>
    <rule name="UnusedFormalParameter">
        <priority>5</priority>
    </rule>
</ruleset>