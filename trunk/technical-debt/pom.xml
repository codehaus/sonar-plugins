<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <parent>
    <groupId>org.codehaus.sonar-plugins</groupId>
    <artifactId>parent</artifactId>
    <version>2</version>
    <relativePath>../parent</relativePath>
  </parent>
  <artifactId>technical-debt</artifactId>
  <name>Technical Debt Sonar plugin</name>
  <version>0.1</version>
  <description>Calculate the technical debt</description>
  <build>
    <plugins>
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-compiler-plugin</artifactId>
        <configuration>
          <source>1.5</source>
          <target>1.5</target>
        </configuration>
      </plugin>
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-jar-plugin</artifactId>
        <configuration>
          <archive>
            <manifestEntries>
              <Plugin-Class>org.sonar.plugins.technicaldebt.TechnicalDebtPlugin</Plugin-Class>
            </manifestEntries>
          </archive>
        </configuration>
      </plugin>
    </plugins>
  </build>
  <dependencies>
    <dependency>
      <groupId>org.codehaus.sonar</groupId>
      <artifactId>sonar-plugin-api</artifactId>
      <version>1.9</version>
    </dependency>
  </dependencies>
  <scm>
    <connection>scm:svn:http://svn.codehaus.org/sonar-plugins/tags/technical-debt-0.1</connection>
    <developerConnection>scm:svn:https://svn.codehaus.org/sonar-plugins/tags/technical-debt-0.1</developerConnection>
    <url>scm:svn:https://svn.codehaus.org/sonar-plugins/tags/technical-debt-0.1</url>
  </scm>
</project>
