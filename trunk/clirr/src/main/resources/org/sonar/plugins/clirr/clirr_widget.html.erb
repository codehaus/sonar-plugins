<% if measure('clirr_total_api_changes') %>
  <div class="yui-gd">
    <div class="yui-u first">
      <div class="dashbox">
        <h3>API Changes</h3>
        <p><span class="big"><%= format_measure('clirr_total_api_changes', :url => url_for_drilldown('clirr_total_api_changes', {:viewer_plugin_key => 'org.sonar.plugins.core.violationsviewer.ViolationsViewer'}), :default => '-')%></span></p>
        <p><span"><%= format_measure('clirr_api_breaks', :suffix => ' API breaks', :url => url_for_drilldown('clirr_api_breaks', {:viewer_plugin_key => 'org.sonar.plugins.core.violationsviewer.ViolationsViewer'}), :default => '-') %></span></p>
        <p><span"><%= format_measure('clirr_api_behavior_changes', :suffix => ' API behavior changes', :url => url_for_drilldown('clirr_api_behavior_changes', {:viewer_plugin_key => 'org.sonar.plugins.core.violationsviewer.ViolationsViewer'}), :default => '-') %></span></p>
        <p><span"><%= format_measure('clirr_new_api', :suffix => ' new API', :url => url_for_drilldown('clirr_new_api', {:viewer_plugin_key => 'org.sonar.plugins.core.violationsviewer.ViolationsViewer'}), :default => '-') %></span></p>
      </div>
    </div>
  </div>
<% end %>